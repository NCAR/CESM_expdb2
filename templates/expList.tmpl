	<div id="NAMED">
	  <table id="cmip6ExpTable">
	    <thead>
	      <th>Case Name</th>
	      <th>Experiment Name</th>
	      <th>Designing MIP</th>
	      <th>Request Date</th>
	      <th>Archive Date</th>
	    </thead>
	    <tbody>
	      [% FOREACH exp IN CMIP6Exps %]
